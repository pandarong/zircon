library("trace-reader") {
  host = true
  sources = [
    "reader.cpp",
    "records.cpp",
  ]
  deps += [
    "$zx/public/gn/config:visibility_hidden",
    "$zx/system/ulib/fbl",
    "$zx/system/ulib/trace-engine",
  ]
  if (is_fuchsia) {
    sources += [ "reader_internal.cpp" ]
    deps += [ "$zx/system/ulib/zxcpp" ]
  }
}
